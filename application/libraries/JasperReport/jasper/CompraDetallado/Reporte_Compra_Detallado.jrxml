	<queryString>
		<![CDATA[Select DCC.IdDetalleComprobanteCompra,CC.IdComprobanteCompra, CC.FechaEmision,Concat(TD.NombreAbreviado, ' ', CC.SerieDocumento, '-', CC.NumeroDocumento)As Documento,
		 Per.RazonSocial, Per.NumeroDocumentoIdentidad,

		if(MND.CodigoMoneda = 'USD' and TD.CodigoTipoDocumento='07',-1*CC.ValorCompraGravado ,(Case When MND.CodigoMoneda = 'USD' then CC.ValorCompraGravado End)) as ValorGravadoD,
	   if(MND.CodigoMoneda = 'USD' and TD.CodigoTipoDocumento='07',-1*CC.ValorCompraNoGravado,(Case When MND.CodigoMoneda = 'USD' then CC.ValorCompraNoGravado End)) As ValorNoGravadoD,
      if(MND.CodigoMoneda = 'USD' and TD.CodigoTipoDocumento='07',-1*CC.IGV,(Case When MND.CodigoMoneda = 'USD' then CC.IGV End)) As IGVD,
      if(MND.CodigoMoneda = 'USD' and TD.CodigoTipoDocumento='07',-1*CC.Total,(Case When MND.CodigoMoneda = 'USD' then CC.Total End)) As TotalD,
		CC.ValorTipoCambio,
		if(MND.CodigoMoneda = 'USD' and TD.CodigoTipoDocumento='07',-1*CC.ValorTipoCambio*CC.ValorCompraGravado,if(TD.CodigoTipoDocumento='07',-1*ValorCompraGravado,
		(Case When MND.CodigoMoneda = 'USD' then CC.ValorTipoCambio * CC.ValorCompraGravado else CC.ValorCompraGravado End))) As ValorGravado,

		if(MND.CodigoMoneda = 'USD' and TD.CodigoTipoDocumento='07',-1*CC.ValorTipoCambio*CC.ValorCompraNoGravado, if(TD.CodigoTipoDocumento='07',-1*ValorCompraNoGravado,
		(Case When MND.CodigoMoneda = 'USD' then CC.ValorTipoCambio * CC.ValorCompraNoGravado else CC.ValorCompraNoGravado End))) As ValorNoGravado,

      if(MND.CodigoMoneda = 'USD' and TD.CodigoTipoDocumento='07',-1*CC.ValorTipoCambio*CC.IGV,if(TD.CodigoTipoDocumento='07',-1*IGV,
		(Case When MND.CodigoMoneda = 'USD' then CC.ValorTipoCambio * CC.IGV else CC.IGV End))) As IGV,

      if(MND.CodigoMoneda = 'USD' and TD.CodigoTipoDocumento='07',-1*CC.ValorTipoCambio*CC.Total, if(TD.CodigoTipoDocumento='07',-1*Total,
		(Case When MND.CodigoMoneda = 'USD' then CC.ValorTipoCambio * CC.Total else CC.Total End))) As Total,

		If(TD.CodigoTipoDocumento='07' or TD.CodigoTipoDocumento='08',
			(select group_concat(DR.NombreAbreviadoDocumentoReferencia," ",
			DR.SerieDocumentoReferencia,"-" ,DR.NumeroDocumentoReferencia separator ", ")
			from documentoreferenciacompra as DR
			where IdComprobanteNota=CC.IdComprobanteCompra and IndicadorEstado='A'),'') as DocReferencia,

      FP.NombreFormaPago as FormaPago, TC.NombreTipoCompra, PE.NombrePeriodo as Periodo,

      DCC.Cantidad as Cantidad,

      (Case When MND.CodigoMoneda='USD' then DCC.CostoUnitarioCalculado end) as CostoUnitarioD,
		(Case when MND.CodigoMoneda='USD' then DCC.CostoItem end) as SubTotalD,
		(Case When MND.CodigoMoneda='USD' then DCC.PrecioUnitario end) as PrecioUnitarioD,
		(Case when MND.CodigoMoneda='USD' then DCC.PrecioItem end) as PrecioSubTotalD,

      (Case When MND.CodigoMoneda='USD' then DCC.CostoUnitarioCalculado*CC.ValorTipoCambio else DCC.CostoUnitarioCalculado end) as CostoUnitario,
		(Case when MND.CodigoMoneda='USD' then DCC.CostoItem*CC.ValorTipoCambio else DCC.CostoItem end) as SubTotal,
		(Case When MND.CodigoMoneda='USD' then DCC.PrecioUnitario*CC.ValorTipoCambio else DCC.PrecioUnitario end) as PrecioUnitario,
		(Case when MND.CodigoMoneda='USD' then DCC.PrecioItem*CC.ValorTipoCambio else DCC.PrecioItem end) as PrecioSubTotal,

		DCC.AbreviaturaUnidadMedida as UnidadMedida, M.CodigoMercaderia, MAR.NombreMarca,
		PRO.IdProducto, PRO.NombreProducto as NombreProducto,
      EMP.CodigoEmpresa, EMP.RazonSocial as NombreEmpresa

      From ComprobanteCompra As CC
      left Join FormaPago As FP on FP.IdFormaPago = CC.IdFormaPago
      left Join TipoDocumento As TD On TD.IdTipoDocumento = CC.IdTipoDocumento
      left Join Proveedor As P On P.IdPersona = CC.IdProveedor
      left Join Persona As Per on Per.IdPersona = P.IdPersona
      left Join Moneda As MND on MND.IdMoneda = CC.IdMoneda
      left join Periodo as PE on PE.IdPeriodo=CC.IdPeriodo
      left join TipoCompra as TC on TC.IdTipoCompra=CC.IdTipoCompra
      left join DocumentoReferenciaCompra as DR on DR.IdComprobanteNota=CC.IdComprobanteCompra
      left Join DetalleComprobanteCompra as DCC On DCC.IdComprobanteCompra= CC.IdComprobanteCompra
      left Join Producto as PRO On PRO.IdProducto=DCC.IdProducto
      left join Mercaderia as M on M.IdProducto=PRO.IdProducto
      left join Modelo as MO on MO.IdModelo=M.IdModelo
      left join Marca as MAR on MAR.IdMarca=MO.IdMarca
    	Cross join Empresa as EMP
		Where (CC.FechaEmision between $P{FechaInicio} and $P{FechaFinal})
			and (CC.IndicadorEstado='A')
			and FP.NombreFormaPago LIKE $P{FormaPago}
			and Per.NumeroDocumentoIdentidad LIKE $P{NumeroDocumentoIdentidad}
			and TC.IdTipoCompra like $P{TipoCompra}
			and TD.IndicadorDocumentoReporteCompra = '1'
		group by DCC.IdDetalleComprobanteCompra
		order by $P!{Orden}]]>
	</queryString>
